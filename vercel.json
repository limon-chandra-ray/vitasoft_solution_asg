{
    "version": "2",
    "build":[
        {
            "src": "collabdev/wsgi.py",
            "use": "@vercal/python",
            "config":{
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.4"
            }
        },{
            "src": "build_files.sh",
            "use": "@vercal/static-build",
            "config": {
                "destDir": "staticfiles_build"
            }

        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "collabdev/wsgi.py"
        }
    ]
}