{
    "version": "2",
    "build":[
        {
            "src": "colladev/wsgi.py",
            "use": "@vercal/python",
            "config":{
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.4"
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "colladev/wsgi.py"
        }
    ]
}